<hibernate-mapping>
    <class name="com.example.demo.Producto" table="Producto">
    <id name="id" type="int" column="id_producto">
        <generator class="native"/>
    </id>

    <property name="nombre" type="string" column="Nombre"/>
    <property name="precio" type="double" column="Precio"/>
    </class>
</hibernate-mapping>